    "axios": "^0.21.4",
    "bulma": "^0.9.3",
    "core-js": "^3.17.3",
    "express": "^4.17.1",
    "mosha-vue-toastify": "^1.0.21",
    "showdown": "^1.9.1",
    "steem": "^0.7.11",
